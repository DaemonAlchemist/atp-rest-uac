{
  "name": "atp-rest-uac",
  "version": "0.2.1",
  "description": "User access control REST endpoints",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run compile && mocha --compilers js:babel-core/register",
    "test-quick": "mocha --compilers js:babel-core/register",
    "compile": "babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile",
    "major": "npm version major && git push && git push origin --tags && npm publish",
    "minor": "npm version minor && git push && git push origin --tags && npm publish",
    "patch": "npm version patch && git push && git push origin --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/DaemonAlchemist/atp-rest-uac.git"
  },
  "author": "Andrew Wittrock",
  "license": "MIT",
  "dependencies": {
    "atp-config": "^0.1.1",
    "jsonwebtoken": "^7.4.3",
    "password-hash": "^1.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^3.5.0"
  }
}
